<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- config global properties -->
    <settings>
        <!-- use jdbc's getGeneratedKeys to get databases's self increasing primary key -->
        <setting name="useGeneratedKeys" value="true" />

        <!-- use column label to replace alias default：true-->
        <setting name="useColumnLabel" value="true"/>

        <!-- enable Camel Case mapping：Table{create_time} -> Entity{createTime} -->
        <setting name="mapUnderscoreToCamelCase" value="true"/>

    </settings>
</configuration>